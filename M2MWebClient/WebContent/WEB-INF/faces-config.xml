<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/content/index.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>authorization</from-outcome>
			<to-view-id>/content/authorization.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>createUser</from-outcome>
			<to-view-id>/content/createuser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>page</from-outcome>
			<to-view-id>/templ/page.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>menu</from-outcome>
			<to-view-id>/content/menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<managed-bean>
		<managed-bean-name>createUser</managed-bean-name>
		<managed-bean-class>com.navigrad.m2m.webclient.entity.CreateUser</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menuController</managed-bean-name>
		<managed-bean-class>com.navigrad.m2m.webclient.controller.template.MenuController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
